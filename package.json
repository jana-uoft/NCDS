{
  "name": "ncds",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "server-start": "nodemon --exec babel-node server/server.js",
    "client-start": "react-scripts start",
    "start": "concurrently --kill-others-on-fail \"yarn server-start\" \"yarn client-start\"",
    "client-test": "react-scripts test --env=jsdom",
    "client-eject": "react-scripts eject",
    "server-build": "babel server --out-dir dist",
    "client-build": "react-scripts build",
    "build": "yarn server-build && yarn client-build"
  },
  "dependencies": {
    "@material-ui/core": "^1.3.1",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.11.0",
    "date-fns": "2.0.0-alpha.7",
    "es6-promise": "^4.2.4",
    "express": "^4.16.2",
    "express-promise-router": "^3.0.2",
    "feedparser-promised": "^1.5.0",
    "helmet": "^3.12.1",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "material-ui-pickers": "^1.0.0-rc.10",
    "mongoose": "^5.1.6",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-drag-sortable": "^1.0.6",
    "react-image-lightbox": "^5.0.0",
    "react-number-format": "^3.4.2",
    "react-redux": "^5.0.7",
    "react-responsive": "^4.1.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-axios-middleware": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-filter": "^0.0.18",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.5.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "nodemon": "^1.17.5"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-runtime"
    ]
  }
}
